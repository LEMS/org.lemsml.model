child_declaration(child) ::= <<
	@XmlElement(name = "<child.name>")
	private <child.type; format="cap"> <child.name>;

>>

child_accessors(prop) ::= "<\n><prop:child_getter()>"

child_getter(child) ::= <<
	public <child.type; format="cap"> get<child.name; format="cap">() {
		return this.<child.name>;
	}
>>
